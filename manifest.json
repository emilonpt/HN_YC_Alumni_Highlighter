{
    "manifest_version": 2,
    "name": "Hacker News YCombinator Alumni Highlighter",
    "version": "1.0",
    "description": "Changes colour of YC Alumni to orange in HN",
    "content_scripts": [
      {
        "matches": ["https://news.ycombinator.com/*"],
        "css": ["styles.css"],
        "js": ["script.js"]
      }
    ],
    "permissions": [
      "https://gist.githubusercontent.com/emilonpt/*"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
}
  }